#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(){
    int a, b, count = 0;
    scanf("%d %d", &a, &b);

    int sum = 0;
    int min;

    for (int i = a; i <= b; i++) {
        for (int j = 2; j <= i; j++) {
            if (i % j == 0) {
                if (i == j) {
                    sum += i;
                    count++;
                    if (count == 1) {
                        min = i;
                    }
                    break;
                }
                else {
                    break;
                }
            }

        }
    }

    if (sum == 0) {
        printf("-1");
    }
    else {
        printf("%d\n%d\n", sum, min);
    }



    return 0;
}
